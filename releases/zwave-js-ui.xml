  <entry>
    <id>https://github.com/zwave-js/zwave-js-ui/releases/tag/v9.16.2</id>
    <title>New release for zwave-js-ui: Release 9.16.2</title>
    <updated>2024-07-22T11:46:10-04:00</updated>
