  <entry>
    <id>https://github.com/n8n-io/n8n/releases/tag/n8n%401.45.1</id>
    <title>New release for n8n: n8n@1.45.1</title>
    <updated>2024-06-12T13:47:33-04:00</updated>
    <author>
      <name>n8n-io/n8n</name>
